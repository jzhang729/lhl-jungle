<section class="sessions-new">

  <header class="page-header">
    <h1>Log In</h1>
  </header>

  <div class="login-form">
    <%= form_tag '/login' do %>
      <%= label_tag :email, "Email" %>
      <%= text_field_tag :email %>

      <%= label_tag :password, "Password" %>
      <%= password_field_tag :password %>

      <%= submit_tag "Submit" %>
    <% end %>
  </div>

</section>
